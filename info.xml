<?xml version="1.0"?>
<extension key="com.joineryhq.fpptaqb" type="module">
  <file>fpptaqb</file>
  <name>FPPTA QuickBooks Sync</name>
  <description>Custom QuickBooks integration for FPPTA</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.fpptaqb</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.fpptaqb</url>
    <url desc="Support">http://joineryhq.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-27</releaseDate>
  <version>0.3.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>Developed by Joinery for FPPTA.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Fpptaqb</namespace>
    <format>23.02.1</format>
    <angularModule>crmFpptaqb</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Fpptaqb_Upgrader</upgrader>
</extension>
